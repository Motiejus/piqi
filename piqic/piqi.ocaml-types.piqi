
.include [ .module piqi.org/piqi ]


.extend [
    (.typedef int uint)
    .with.ocaml-type "int"
]


.extend [
    (.typedef int32 uint32)
    .with.ocaml-type "int32"
]


.extend [
    (.typedef int64 uint64)
    .with.ocaml-type "int64"
]


.extend [
    (.typedef float64 float32)
    .with.ocaml-type "float"
]


.custom-field ocaml-type

